# This serves as an example Podfile for installing PubNub with Cocoapods
workspace 'AppleExamples'

use_frameworks!

target 'ClientInAppDelegateObjC' do
	project 'Objective-C/ClientInAppDelegateObjC/ClientInAppDelegateObjC'
	platform :ios, '10.0'
	pod 'PubNub'
end

target 'PushNotificationsObjC' do
	project 'Objective-C/PushNotificationsObjC/PushNotificationsObjC'
	platform :ios, '10.0'
	pod 'PubNub'
end

target 'OldPushNotificationsObjC' do
	project 'Objective-C/OldPushNotificationsObjC/OldPushNotificationsObjC'
	platform :ios, '9.0'
	pod 'PubNub'
end

target 'ClientAsSingletonObjC' do
	project 'Objective-C/ClientAsSingletonObjC/ClientAsSingletonObjC'
	platform :ios, '8.0'
	pod 'PubNub'
end

target 'ClientInAppDelegateSwift' do
	project 'Swift/ClientInAppDelegateSwift/ClientInAppDelegateSwift'
	platform :ios, '10.0'
	pod 'PubNub'
end

target 'PushNotificationsSwift' do
	project 'Swift/PushNotificationsSwift/PushNotificationsSwift'
	platform :ios, '10.0'
	pod 'PubNub'
end

target 'OldPushNotificationsSwift' do
	project 'Swift/OldPushNotificationsSwift/OldPushNotificationsSwift'
	platform :ios, '9.0'
	pod 'PubNub'
end

target 'ClientAsSingletonSwift' do
	project 'Swift/ClientAsSingletonSwift/ClientAsSingletonSwift'
	platform :ios, '8.0'
	pod 'PubNub'
end

target 'MapboxObjC' do
	project 'Objective-C/MapboxObjC/MapboxObjC'
	platform :ios, '8.0'
	pod 'PubNub'
	pod 'Mapbox-iOS-SDK', '~> 3.3'
end

target 'MapboxSwift' do
	project 'Swift/MapboxSwift/MapboxSwift'
	platform :ios, '8.0'
	pod 'PubNub'
	pod 'Mapbox-iOS-SDK', '~> 3.3'
end
